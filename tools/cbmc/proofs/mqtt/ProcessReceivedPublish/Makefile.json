{
    "ENTRY": "ProcessReceivedPublish",

    "MAX_SUBSCRIPTIONS": "2",
    "MAX_TOPIC_LENGTH": "10",

    "MAX_SUBSCRIPTIONS_PLUS_1": "__eval {MAX_SUBSCRIPTIONS} + 1",
    "MAX_TOPIC_LENGTH_PLUS_1": "__eval {MAX_TOPIC_LENGTH} + 1",

    "CBMCFLAGS":
    [
        "--unwind 1",
        "--nondet-static",

	"--unwindset prvInvokeSubscriptionCallbacks.0:{MAX_SUBSCRIPTIONS_PLUS_1},prvInvokeSubscriptionCallbacks.1:{MAX_SUBSCRIPTIONS_PLUS_1},memcmp.0:{MAX_TOPIC_LENGTH_PLUS_1},allocate_MQTTSubscriptionManager.0:{MAX_SUBSCRIPTIONS_PLUS_1},is_valid_MQTTSubscriptionManager.0:{MAX_SUBSCRIPTIONS_PLUS_1},prvDoesTopicMatchTopicFilter.0:{MAX_TOPIC_LENGTH_PLUS_1},prvDoesTopicMatchTopicFilter.1:{MAX_TOPIC_LENGTH_PLUS_1}"
    ],
    "OBJS":
    [
        "$(ENTRY)_harness.goto",
	"util.goto",
	"mqtt_type.goto",
        "$(FREERTOS)/lib/mqtt/aws_mqtt_lib.goto"
    ],
    "DEF":
    [
	"MQTT_BUFFER_USER_DATA_LENGTH=100",
	"mqttconfigSUBSCRIPTION_MANAGER_MAX_SUBSCRIPTIONS={MAX_SUBSCRIPTIONS}",
	"mqttconfigSUBSCRIPTION_MANAGER_MAX_TOPIC_LENGTH={MAX_TOPIC_LENGTH}"
    ]
}
